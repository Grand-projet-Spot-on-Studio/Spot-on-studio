{% extends 'base.html.twig' %}

{% block body  %}

    {% for studio in studios %}
        <div>
            <h1>{{ studio.name }}</h1>
            <p>{{ studio.adress }}</p>
            <p>{{ studio.phone }}</p>
            <p>{{ studio.zipcode }}</p>
            <p>{{ studio.city }}</p>
            <p>{{ studio.email }}</p>
        </div>
    {% endfor %}
    {{ knp_pagination_render(studios) }}

{% endblock %}